{
  "manifest_version": 3,
  "name": "WhereDidMyTimeGo",
  "version": "1.2",
  "description": "Track your website usage and see where your time really goes. Complete time analytics with insights, limits, and session tracking.",
  "permissions": ["tabs", "storage", "alarms"],
  "host_permissions": ["<all_urls>"],
  "incognito": "split",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+T",
        "mac": "Alt+T"
      },
      "description": "Open WhereDidMyTimeGo popup"
    },
    "open-dashboard": {
      "suggested_key": {
        "default": "Alt+S",
        "mac": "Alt+S"
      },
      "description": "Open dashboard in new tab"
    }
  }
}
